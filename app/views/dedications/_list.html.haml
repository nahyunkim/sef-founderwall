#filterrific_results
  %div
    = page_entries_info dedications
    
  %table
    %tr
      %th Donor
      %th Dedication
      %th Hosptial
    - dedications.each do |d|
      %tr
        %td= d.donor.first_name + " " +  d.donor.last_name
        %td= d.dedication
        %td= d.hospital.name
        
.pagination
  = will_paginate dedications
